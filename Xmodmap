clear shift
clear lock
clear control
clear mod5
keycode  22 = Delete NoSymbol Delete
keycode  50 = Home NoSymbol Home
keycode  62 = End NoSymbol End
keycode  66 = BackSpace BackSpace BackSpace BackSpace
keycode 105 = ISO_Level3_Shift NoSymbol ISO_Level3_Shift
keycode 108 = Shift_L NoSymbol Shift_L
keycode 119 = Caps_Lock NoSymbol Caps_Lock
add shift = Shift_L
add lock = Caps_Lock
add control = Control_L
add mod5 = ISO_Level3_Shift Mode_switch
